{
  "name": "apollo-server-typescript",
  "version": "1.0.0",
  "description": "Apollo Server with TypeScript sample",
  "main": "index.js",
  "keywords": [
    "graphql",
    "typescript",
    "apollo",
    "apollo-server"
  ],
  "author": "Iurii Okhmat",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql",
    "start": "node 'dist/server.js'",
    "build": "npm run clean && tsc -p . && ncp src/schema/types dist/schema/types",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^5.0.0",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/helmet": "0.0.45",
    "@types/node": "^13.13.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "0.0.4",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^5.1.1",
    "graphql": "^15.0.0",
    "helmet": "^3.22.0",
    "http": "0.0.1-security",
    "merge-graphql-schemas": "^1.7.8",
    "ncp": "^2.0.0",
    "nocache": "^2.1.0",
    "redirect-ssl": "^1.4.1"
  }
}
